    function situacionAgricola(id) {
        let idl = id.substring(21, id.lenght);

        let precio_venta_, produccion_esperada_porcentaje_comercial_, total;

        precio_venta_ = $('#precio_venta_' + idl).val();
        produccion_esperada_ = $('#produccion_esperada_' + idl).val();
        porcentaje_comercial_ = $('#porcentaje_comercial_' + idl).val();


        if (precio_venta_.length == 0 || precio_venta_ == "" ||
            produccion_esperada_.length == 0 || produccion_esperada_ == "" ||
            porcentaje_comercial_.length == 0 || porcentaje_comercial_ == "") {

            $('#agricolaError').html('<span class="badge badge-danger">Error faltan datos</span>');
        } else {
            precio_venta_ = parseInt(precio_venta_);
            produccion_esperada_ = parseInt(produccion_esperada_);
            porcentaje_comercial_ = parseInt(porcentaje_comercial_);
            porcentaje_comercial_ = porcentaje_comercial_ * 0.01;

            total = precio_venta_ * produccion_esperada_ * porcentaje_comercial_;
            total = parseFloat(total).toFixed(2);
            $('#agricolaError').html(' ');

            $('#total_cultivo_' + idl).val(total);

        }


    }
